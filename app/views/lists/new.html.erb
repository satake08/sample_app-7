<h1>新規投稿</h1>
<!-- form_with部分 -->
<!-- form_withにオプション追記 -->
<%= form_with model: @list, url: '/lists', method: :post do |f| %>
<!-- ↑こちらに書き換え%= form_with model: @list do |f| %>←反対側にもある -->
<!-- ↑こちらに書き換え%= form_with model: List.new do |f| %>←反対側にもある -->
  <h4>タイトル</h4>
  <%= f.text_field :title %>

  <h4>本文</h4>
  <%= f.text_area :body %>

  <!-- 追加部分 -->
  <h4>画像</h4>
  <%= f.file_field :image, accept: "image/*" %>

  <%= f.submit '投稿' %>
<% end %>